<div class="form-group">
	<label for="email">Email</label>
	<input required value="<%= user.email %>" type="text" name="email" id="email" class="form-control">
</div>

<div class="form-group">
	<label for="username">Username</label>
	<input required value="<%= user.username %>" type="text" name="username" id="username" class="form-control">
</div>

<div class="form-group">
	<label for="password">Password</label>
	<input required value="<%= user.password %>" type="text" name="password" id="password" class="form-control">
</div>


<a href="/" class="btn btn-secondary">Cancel</a>

